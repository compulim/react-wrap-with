{
  "name": "react-wrap-with",
  "version": "0.0.0-0",
  "description": "Render middleware for React.",
  "files": [
    "./lib/*"
  ],
  "exports": {
    ".": {
      "import": "./lib/esmodules/index.js",
      "require": "./lib/commonjs/index.js"
    },
    "./wrapWith": {
      "import": "./lib/esmodules/wrapWith.js",
      "require": "./lib/commonjs/wrapWith.js"
    }
  },
  "main": "./lib/commonjs/index.js",
  "typings": "./lib/types/index.d.ts",
  "scripts": {
    "build": "npm run build:babel && npm run build:typescript",
    "build:babel": "npm run build:babel:commonjs && npm run build:babel:esmodules",
    "build:babel:commonjs": "babel src --config-file ./babel.commonjs.config.json --extensions .ts,.tsx --out-dir ./lib/commonjs/",
    "build:babel:esmodules": "babel src --config-file ./babel.esmodules.config.json --extensions .ts,.tsx --out-dir ./lib/esmodules/",
    "build:typescript": "tsc --project ./src/tsconfig.declaration.json",
    "precommit": "eslint ./src/",
    "prepack": "cp ../../CHANGELOG.md . && cp ../../LICENSE . && cp ../../README.md .",
    "start": "esbuild --bundle --outfile=./public/main.js --servedir=./public --sourcemap ./scenarios/index.jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compulim/react-wrap-with.git"
  },
  "keywords": [
    "react",
    "react-hook",
    "react-hooks"
  ],
  "author": "William Wong (https://github.com/compulim)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compulim/react-wrap-with/issues"
  },
  "homepage": "https://github.com/compulim/react-wrap-with#readme",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^17.0.52",
    "esbuild": "^0.17.5",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.20.7"
  }
}
